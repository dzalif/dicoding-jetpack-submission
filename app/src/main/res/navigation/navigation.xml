<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.kucingselfie.dicodingjetpacksubmission1.ui.home.HomeFragment"
        android:label="home_fragment"
        tools:layout="@layout/home_fragment" >
        <action
            android:id="@+id/action_homeFragment_to_movieFragment"
            app:destination="@id/movieFragment" />
    </fragment>
    <fragment
        android:id="@+id/movieFragment"
        android:name="com.kucingselfie.dicodingjetpacksubmission1.ui.movie.MovieFragment"
        tools:layout="@layout/movie_fragment"
        android:label="Movie" />
    <fragment
        android:id="@+id/TVShowFragment"
        android:name="com.kucingselfie.dicodingjetpacksubmission1.ui.tvshow.TVShowFragment"
        android:label="TVShowFragment" />
</navigation>